{
  "manifest_version": 3,
  "name": "__MSG_extension_name__",
  "description": "__MSG_extension_description__",
  "version": "4.1",
  "icons": {
    "128": "icon_128.png"
  },
  "default_locale": "en",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
